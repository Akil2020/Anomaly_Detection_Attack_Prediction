      "datasource": {
        "type": "postgres",
        "uid": null,
        "name": "PostgresInfra"
      },
      "targets": [
        {
          "refId": "B",
          "rawSql": "SELECT $__timeGroupAlias(\"ts\",'5m'), avg(NULLIF(\"anomaly_score\", '')::double precision) as value FROM predictions_staging WHERE NULLIF(\"anomaly_score\", '')::double precision IS NOT NULL GROUP BY 1 ORDER BY 1;",
