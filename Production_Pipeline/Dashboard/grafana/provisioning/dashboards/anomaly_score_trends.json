{
  "uid": "anomaly_score_trends",
  "title": "Anomaly Score Trends",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Anomaly Score (raw)",
      "datasource": {
        "type": "postgres",
        "uid": null,
        "name": "PostgresInfra"
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT $__time(\"ts\"), NULLIF(\"anomaly_score\", '')::double precision as anomaly_score FROM predictions_staging WHERE NULLIF(\"anomaly_score\", '')::double precision IS NOT NULL ORDER BY 1;",
          "format": "time_series"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
